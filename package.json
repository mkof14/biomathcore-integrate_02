{
  "name": "biomath-core-ver-1.2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "lsof -ti:3000 | xargs -r kill -9 && NEXT_DISABLE_ESLINT=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "prisma": "prisma",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "seed": "bash scripts/seed-reports.sh",
    "format": "prettier --write .",
    "check:frozen": "node scripts/check-frozen.js",
    "dev:fast": "NEXT_DISABLE_ESLINT=1 next dev",
    "smoke": "bash scripts/smoke.sh",
    "smoke:dev": "bash scripts/dev-smoke.sh",
    "seed:catalog": "tsx prisma/seed_catalog.ts",
    "seed:questionnaires": "tsx prisma/seed_questionnaires.ts",
    "seed:dynamic-rules": "tsx prisma/seed_dynamic_rules.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init_or_sync",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "node prisma/seed.mjs || true",
    "lint:strict": "eslint src --ext .ts,.tsx --max-warnings=0",
    "cleanup:api": "node tools/api-surface-cleanup.mjs",
    "lint:active": "eslint src/lib src/app/api --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test:smoke": "playwright test -g @smoke",
    "status:curl": "curl -sS http://localhost:3000/api/status | jq .",
    "metrics:curl": "curl -sS http://localhost:3000/api/metrics | jq .",
    "cleanup:report": "node tools/tighten-report-engine.mjs",
    "test:unit": "vitest run --reporter=dot",
    "test:unit:watch": "vitest",
    "test:all": "pnpm run test:unit && pnpm run test:e2e",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "freeze:check": "node tools/freeze-guard.mjs --ci",
    "prepush": "pnpm run typecheck && pnpm run test:unit && PLAYWRIGHT_BASE_URL=http://localhost:3000 pnpm exec playwright test -c tests/e2e/smoke.config.ts -g \"@smoke\"",
    "prepare": "husky",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down -v",
    "db:wait": "until docker compose exec -T db pg_isready -U biomath -d biomath_core; do sleep 1; done",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev --name sync",
    "test:e2e": "playwright test --config=tests/e2e/smoke.config.ts",
    "fast": "pnpm install && pnpm prisma generate && pnpm prisma migrate dev --name sync || true && pnpm run db:seed || true && PLAYWRIGHT_BASE_URL=http://localhost:3000 pnpm run test:all && pnpm run dev"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@json2csv/plainjs": "7.0.6",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@react-pdf/renderer": "^4.3.0",
    "@stripe/stripe-js": "3.5.0",
    "bcrypt": "6.0.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "4.1.0",
    "firebase": "12.1.0",
    "json2csv": "6.0.0-alpha.2",
    "jspdf": "3.0.2",
    "jszip": "3.10.1",
    "lucide-react": "0.541.0",
    "luxon": "^3.7.1",
    "nanoid": "5.1.5",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "pdf-lib": "^1.17.1",
    "rate-limiter-flexible": "7.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.62.0",
    "react-markdown": "^10.1.0",
    "react-toastify": "11.0.5",
    "recharts": "^3.1.2",
    "remark-gfm": "^4.0.1",
    "stripe": "16.12.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "uuid": "11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.55.0",
    "@types/node": "20.12.12",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "^9.0.0",
    "postcss": "^8.5.6",
    "prettier": "3.3.3",
    "prisma": "^6.14.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "5.5.4",
    "vitest": "^3.2.4"
  },
  "overmind": {
    "dummy": "0"
  }
}